{
  "name": "FindSuperman",
  "version": "0.0.1",
  "author": "Patrick Catanzariti",
  "description": "Code designed to find the elusive red blue blur",
  "dependencies": {
    "react": "latest",
    "page": "latest"
  },
  "devDependencies": {
    "browserify": "latest",
